                <input type="tel" class="form-control" id="phone" name="phone" placeholder="+91-1234567890"
                       pattern="[91]-[0-9]{10}" required><br>



                <!-- Email Address -->
                <label for="email" class="form-label">Email address</label>
                <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="evan.donohue@gmail.com" /><br>

